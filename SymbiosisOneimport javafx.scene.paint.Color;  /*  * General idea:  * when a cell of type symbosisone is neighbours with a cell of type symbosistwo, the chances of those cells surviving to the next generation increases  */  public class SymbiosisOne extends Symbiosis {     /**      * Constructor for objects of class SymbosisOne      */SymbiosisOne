import javafx.scene.paint.Color;

/*
 * General idea:
 * when a cell of type symbosisone is neighbours with a cell of type symbosistwo, the chances of those cells surviving to the next generation increases
 */

public class SymbiosisOne extends Symbiosis
{
    /**
     * Constructor for objects of class SymbosisOne
     */
    public SymbiosisOne(Field field, Location location, Color col)
    {
        super(field, location, col);
    }
}
